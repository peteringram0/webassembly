  "license": "MIT",
  "scripts": {
    "serve": "go run ./server/server.go",
    "build:go": "cd go && GOOS=js GOARCH=wasm go build -o ./main.wasm",
    "build:test1": "wa compile c/main.c -o ./../main.wasm",
    "build:test2": "emcc ./c/main.c -s WASM=1 -s SIDE_MODULE=1 -o ./c/main.wasm",
    "build:c": "emcc ./c/main.c -s SIDE_MODULE=1 -o ./c/main.wasm"
  }
}
